<!-- account / orders -->
<div x-cloak x-show="path == '#/orders'">
  <div class="mx-auto max-w-[50rem] space-y-8">
    <menu aria-label="{{ 'common.aria_label.breadcrumbs' | t }}" class="inline-flex flex-wrap gap-2 text-body-sm">
      <li class="link">
        <a aria-label="{{ 'sections.main_account.start' | t }}" href="{{ routes.account_url | append: '#/main' }}" @click="path = '#/main'">
          {{ 'sections.main_account.start' | t }}
        </a>
      </li>
      <li>
        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd" clip-rule="evenodd" d="M7.20938 14.7698C6.92228 14.4713 6.93159 13.9965 7.23017 13.7094L11.1679 10L7.23017 6.29062C6.93159 6.00353 6.92228 5.52875 7.20938 5.23017C7.49647 4.93159 7.97125 4.92228 8.26983 5.20937L12.7698 9.45937C12.9169 9.60078 13 9.79599 13 10C13 10.204 12.9169 10.3992 12.7698 10.5406L8.26983 14.7906C7.97125 15.0777 7.49647 15.0684 7.20938 14.7698Z" fill="#6B7280"/>
        </svg>
      </li>
      <li class="link">
        <span class="font-semibold text-primary">{{ 'sections.main_account.orders' | t }}</span>
      </li>
    </menu>
    <div>
      <h1 class="title font-serif text-h3 font-bold md:text-h2">{{ 'sections.main_account.orders' | t }}</h1>
      <p class="description mt-2 w-full text-body-base">
        {{ 'sections.main_account.product_actions' | t }}
      </p>
    </div>
    <div 
      x-data="
        {
          orderPage: 1,
          loading: false,
          showMore(){
            this.loading = true;
            currentUrl = window.location.href.replace('#/orders','') + `?page=` + (this.orderPage+1) + `#/orders`;
            fetch(currentUrl).then(response => {
              if (response.status !== 200) {
                throw new Error(response.statusText)
              } else {
                return response.text()
              }
            }).then(cartData => {
              const orders = new DOMParser().parseFromString(cartData, 'text/html').getElementById('orders-container-{{ section.id }}')
              const ul = document.getElementById('orders-container-{{ section.id }}')

              const orderCards = Array.from(orders.children)

              orderCards.forEach((order) => ul.appendChild(order))

              this.orderPage = this.orderPage + 1
              this.loading = false
            })
            .catch((error) => {
              console.error(error)
              this.loading = false
            })
          }
        }
      " 
      class="space-y-6"
    >
      <div
        x-cloak
        x-show="loading"
        class="fixed inset-0 z-50 flex items-center justify-center overflow-hidden bg-black/40 pt-0 text-white"
      >
        {% render 'loading-spinner' %}
      </div>
      {% paginate customer.orders by 10 %}
        {% assign pages_size = paginate.pages %}

        <div id="orders-container-{{ section.id }}">
          {% if customer.orders.size == 0 %}
            <div class="card flex flex-col-reverse items-center justify-between gap-6 border border-neutral-200 px-6 py-14 md:flex-row">
              <div class="flex flex-col gap-2">
                <h4 class="card-title text-center font-serif text-h5 font-bold md:text-left">{{ 'sections.main_account.empty_order' | t }}</h4>
                <p class="card-description text-center text-gray-700 md:text-left">{{ 'sections.main_account.status_here' | t }}</p>
              </div>
              {% capture icon %}
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 24 24"
                  stroke-width="1.5"
                  stroke="currentColor"
                  class="icon card-icon"
                >
                  <path d="M3.41334 3.03994C2.38241 3.03994 1.54667 3.87568 1.54667 4.9066V5.65328C1.54667 6.6842 2.3824 7.51996 3.41333 7.51996H20.5867C21.6176 7.51996 22.4533 6.6842 22.4533 5.65328V4.9066C22.4533 3.87568 21.6176 3.03994 20.5867 3.03994H3.41334Z" fill="#6B7280"/>
                  <path fill-rule="evenodd" clip-rule="evenodd" d="M3.12641 9.01328L3.66378 18.1486C3.75664 19.7273 5.06392 20.96 6.64528 20.96H17.3544C18.9358 20.96 20.2431 19.7273 20.3359 18.1486L20.8733 9.01328H3.12641ZM9.23204 11.8453C9.5236 11.5537 9.9964 11.5537 10.288 11.8453L12 13.5573L13.712 11.8453C14.0036 11.5537 14.4764 11.5537 14.768 11.8453C15.0596 12.1369 15.0596 12.6096 14.768 12.9012L13.056 14.6133L14.768 16.3253C15.0596 16.6169 15.0596 17.0896 14.768 17.3812C14.4764 17.6728 14.0036 17.6728 13.712 17.3812L12 15.6692L10.288 17.3812C9.9964 17.6728 9.5236 17.6728 9.23204 17.3812C8.94044 17.0896 8.94044 16.6169 9.23204 16.3253L10.944 14.6133L9.23204 12.9012C8.94044 12.6096 8.94044 12.1369 9.23204 11.8453Z" fill="#6B7280"/>
                </svg>
              {% endcapture %}
              <div class="[&>div>svg]:stroke-none">
                {% render 'avatar', size: 'xlarge', icon: icon %}
              </div>
            </div>
          {% else %}
              {% for order in customer.orders %}
                <div class="card flex flex-col gap-6 border border-neutral-200 p-6">
                  <div class="flex flex-col gap-1">
                    <div class="flex items-center justify-between gap-2">
                      <h3 class="order-title font-serif text-h5 font-bold">{{ 'sections.main_account.order' | t }} {{ order.name }}</h3>
                      {% render 'order-status-label', order: order %}
                    </div>
                    <div class="flex justify-between gap-2">
                      <p class="order-created-at">{{ order.created_at | time_tag: format: 'date' }}</p>
                      <span class="order-item-count">{{ order.item_count }} {{ order.item_count | pluralize: 'item', 'itens' }}</span>
                    </div>
                  </div>
                  <div class="flex flex-col space-y-6">
                    <hr class="border-t border-neutral-200">
                    {% for line_item in order.line_items %}
                      {% render 'order-line-item', line_item: line_item %}
                    {% endfor %}
                    <hr class="border-t border-neutral-200">
                  </div>
                  <div class="flex flex-col gap-6">
                    <div class="text flex justify-end gap-2 lining-nums tabular-nums">
                      {{ 'sections.main_account.total' | t }}&nbsp;<strong class="order-price">{{ order.total_price | money }}</strong>
                    </div>
                    <div class="flex flex-col gap-2">
                      <a aria-label="{{ 'sections.main_account.see_detail' | t }}" href="{{ order.customer_url | default: '#' }}" class="button button-outline button-primary w-full md:w-auto">
                        {{ 'sections.main_account.see_detail' | t }}
                      </a>
                    </div>
                  </div>
                </div>
              {% endfor %}
          {% endif %}
        </div>
        <div class="mt-12 flex w-full items-center justify-center">
          <button
            class="button button-outline"
            @click="showMore"
            :class="orderPage != {{ pages_size }} || 'hidden'"
          >
            {{ 'sections.main_search.view_more' | t }}
          </button>
        </div>
      {% endpaginate %}
    </div>
  </div>
</div>
