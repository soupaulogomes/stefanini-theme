    settings.alternative_store_logo
    | image_url: width: 600
    | image_tag: loading: loading, fetchpriority: fetchpriority, class: logo_classes
  }}
{% elsif settings.store_logo %}
  {{ settings.store_logo | image_url: width: 600 | image_tag: class: logo_classes }}
