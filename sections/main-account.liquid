<div x-cloak x-show="path == '#/password'">
  <div class="mx-auto max-w-[50rem] space-y-8">
    <menu aria-label="{{ 'common.aria_label.breadcrumbs' | t }}" class="inline-flex flex-wrap gap-2 text-body-sm">
      <li class="link">
        <a aria-label="{{ 'sections.main_account.start' | t }}" href="{{ routes.account_url | append: '#/password' }}" @click="path = '#/password'">
          {{ 'sections.main_account.start' | t }}
        </a>
      </li>
      <li>
        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path fill-rule="evenodd" clip-rule="evenodd" d="M7.20938 14.7698C6.92228 14.4713 6.93159 13.9965 7.23017 13.7094L11.1679 10L7.23017 6.29062C6.93159 6.00353 6.92228 5.52875 7.20938 5.23017C7.49647 4.93159 7.97125 4.92228 8.26983 5.20937L12.7698 9.45937C12.9169 9.60078 13 9.79599 13 10C13 10.204 12.9169 10.3992 12.7698 10.5406L8.26983 14.7906C7.97125 15.0777 7.49647 15.0684 7.20938 14.7698Z" fill="#6B7280"/>
        </svg>
      </li>
      <li class="link">
        <span class="font-semibold text-primary">{{ 'sections.main_account.security' | t }}</span>
      </li>
    </menu>
    <div>
      <h1 class="title font-serif text-h3 font-bold md:text-h2">{{ 'sections.main_account.security' | t }}</h1>
      <p class="description mt-2 w-full text-body-base">
        {{ 'sections.main_account.manage_password' | t }}
      </p>
    </div>
    <div class="card flex flex-col gap-6 border border-neutral-200 p-6">
      <div>
        <h3 class="card-title font-serif text-h5 font-bold">{{ 'sections.main_account.change_password' | t }}</h3>
        <p class="card-description mt-2 w-full text-body-base">
          {{ 'sections.main_account.new_password_caracteres' | t }}
        </p>
      </div>
      <div
        x-data="
        {
          loading: false,
          text : {
            security: '{{ 'sections.main_account.security' | t }}',
            wrong_password: '{{ 'sections.main_account.wrong_password' | t }}',
            new_password_caracteres: '{{ 'sections.main_account.new_password_caracteres' | t }}',
            success_password: '{{ 'sections.main_account.success_password' | t }}',
            error_password: '{{ 'sections.main_account.error_password' | t }}'
          },
          customer: {
            id: {{ customer.id }},
            password: '',
            password_confirmation: '',
          },
          handleChangePassword: function (event) {
            if (this.customer.password != this.customer.password_confirmation) {
              $dispatch('show-notification', { type: 'error', title: this.text.security, description: this.text.wrong_password })
              return;
            }

            if (this.customer.password.length < 7) {
              $dispatch('show-notification', { type: 'error', title: this.text.security, description: this.text.new_password_caracteres })
              return;
            }

            this.loading = true;

            $store.customer.update(this.customer)
            .then(response => {
              this.loading = false;

              if (response.status == 204) {
                this.password = '';
                this.password_confirmation = '';

                $dispatch('show-notification', { type: 'success', title: this.text.security, description: this.text.success_password })
              }
              else {
                $dispatch('show-notification', { type: 'error', title: this.text.security, description: this.text.error_password })
              }
            })
            .catch(error => {
              this.loading = false;

              $dispatch('show-notification', { type: 'error', title: this.text.security, description: this.text.error_password })
            });
          },
        }
      ">
        {% form 'reset_customer_password' %}
          <div class="flex flex-col gap-6">
            <div class="flex w-full flex-col gap-4 md:flex-row">
              <div class="flex flex-1 flex-col gap-2">
                <label class="text-body-sm" for="customer_password">{{ 'sections.main_account.new_password' | t }}</label>
                <input
                  id="customer_password"
                  type="password"
                  required
                  name="customer[password]"
                  x-model="customer.password"
                  class="input h-12"
                  autocomplete="new-password"
                  placeholder="{{ 'sections.main_account.new_password' | t }}"
                  minlength="7"
                >
              </div>
              <div class="flex flex-1 flex-col gap-2">
                <label class="text-body-sm" for="customer_password_confirmation">{{ 'sections.main_account.confirm_password' | t }}</label>
                <input
                  id="customer_password_confirmation"
                  type="password"
                  required
                  name="customer[password_confirmation]"
                  x-model="customer.password_confirmation"
                  class="input h-12"
                  autocapitalize="characters"
                  autocomplete="new-password"
                  placeholder="{{ 'sections.main_account.confirm_password' | t }}"
                  minlength="7"
                >
              </div>
            </div>
            <div class="flex flex-col-reverse justify-between gap-2 md:flex-row">
              <a
                aria-label="{{ 'sections.main_account.cancel' | t }}"
                href="{{ routes.account_url | append: '#/profile' }}"
                @click="path = '#/profile'"
                :disabled="loading"
                class="button button-outline button-primary w-full md:w-auto"
              >
                <span>
                  {{ 'sections.main_account.cancel' | t }}
                </span>
              </a>
              <button @click.prevent="handleChangePassword()" class="button button-primary">
                {{ 'sections.main_account.save_new_password' | t }}
              </button>
            </div>
          </div>
        {% endform %}
      </div>
    </div>
  </div>
</div>
{% endcapture %}

